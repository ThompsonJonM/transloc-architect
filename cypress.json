{
  "reporter": "mocha-multi-reporters",
  "reporterOptions": {
    "configFile": "reporter.config.json"
  },

  "screenshotsFolder": "cypress/output/screenshots",
  "videosFolder": "cypress/output/videos",

  "viewportWidth": 1440,
  "viewportHeight": 900,

  "waitForAnimations": true,

  "defaultCommandTimeout": 10000,

  "ignoreTestFiles": "*.js"
}
